{
    "name": "dspacelabs/http-client",
    "description": "Simple PHP HTTP Client Library",
    "license": "MIT",
    "keywords": ["psr7", "curl"],
    "homepage": "https://github.com/dSpaceLabs/http-client",
    "support": {
        "issues": "https://github.com/dSpaceLabs/http-client/issues",
        "source": "https://github.com/dSpaceLabs/http-client",
        "docs": "https://github.com/dSpaceLabs/http-client"
    },
    "require": {
        "dspacelabs/http-message": "^0.2@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "phing/phing": "^2.16"
    },
    "autoload": {
        "psr-4": { "Dspacelabs\\Component\\Http\\Client\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Dspacelabs\\Component\\Http\\Client\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
